<%- include("partials/header.ejs") -%>
    <h1>Compose</h1>
    <form action="/compose" method="post">
        <div class="form-group">
            <label for="postTitle">Title</label><br>
            <input id="postTitle" class="form-control" type="text" name="postTitle"><br>
    
            <label for="postDescription">Post</label><br>
            <textarea id="postDescription" class="form-control" name="postDescription" rows="5"></textarea><br>
        </div>
        <button class="btn btn-primary" type="submit" name="button">Publish</button>
    </form>
